package tictactoe;

import java.util.Scanner;

public class Main {
	public String getWiner(int[][] q) {
		int numofX = 0;
		int numofY = 0;
		boolean flag = true;
		for (int i = 0; i < q.length; i++) {
			for (int j = 0; j < q.length; j++) {
				if (q[i][j] == 1) {
					numofX++;
				} else {
					numofY++;
				}
			}
			if (numofX == q.length || numofY == q.length) {
				flag = false;
				break;
			} else {
				numofX = 0;
				numofY = 0;
			}
		}
		if (flag) {
			numofX = 0;
			numofY = 0;
			for (int i = 0; i < q.length; i++) {
				for (int j = 0; j < q.length; j++) {
					if (q[j][i] == 1) {
						numofX++;
					} else {
						numofY++;
					}
				}
				if (numofX == q.length || numofY == q.length) {
					flag = false;
					break;
				} else {
					numofX = 0;
					numofY = 0;
				}
			}
		}
		if (flag) {
			numofX = 0;
			numofY = 0;
			for (int i = 0; i < q.length; i++) {
				if (q[i][i] == 1) {
					numofX++;
				} else {
					numofY++;
				}
			}
			if (numofX == q.length || numofY == q.length) {
				flag = false;
			}
		}
		if (flag) {
			numofX = 0;
			numofY = 0;
			for (int i = 0; i < q.length; i++) {
				if (q[i][q.length - 1 - i] == 1) {
					numofX++;
				} else {
					numofY++;
				}
			}

		}
		if (numofX == q.length) {
			return "X";
		} else if (numofY == q.length) {
			return "Y";
		}else{
//		if (numofX != q.length && numofY != q.length) {
			return "NIL";
		}

	}

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = Integer.parseInt(in.nextLine());
		int[][] jzq = new int[n][n];
		int i = 0;
		String line;
		while (n != 0 && i != n) {

			line = in.nextLine();
			String[] qz = line.split(" ");

			for (int j = 0; j < jzq.length; j++) {
				jzq[i][j] = Integer.parseInt(qz[j]);
			}

			i++;

			n--;
		}

		in.close();
		System.out.println(new Main().getWiner(jzq));
	}

}
