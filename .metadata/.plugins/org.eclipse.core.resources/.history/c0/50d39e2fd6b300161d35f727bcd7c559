package mileage;

import java.util.HashMap;
import java.util.Map;

public class CityHelper {

	private Map<String,City> list = new HashMap<String,City>();
//	得到城市数目
	public int getCityNum(String s)
	{
		return 0;
	}
//	增加城市
	public void addCity(City c)
	{
		list.add(c);
	}
//	增加目的地
	public void addDest(City c,String dest,int mileage)
	{
		c.addDest(dest, mileage);
	}
	public int getMileage(String c,String dest)
	{
		return list..getMileage(dest);
	}
}
