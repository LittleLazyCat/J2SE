package fahrenheit2centigrade;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in =new Scanner(System.in);
		try
		{
			float F = in.nextFloat();
			F=(F-32)/(9/5.0f);
			System.out.println((int)F);
			
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
	}

}
