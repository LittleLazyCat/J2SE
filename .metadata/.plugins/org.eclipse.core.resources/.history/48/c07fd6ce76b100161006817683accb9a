package additionofpolynomial;

import java.util.Map;

//多项式加法
/*
* 输入样例：
6 2
5 3
3 12
1 6
0 20
6 2
5 3
2 12
1 6
0 20

输出样例：
4x6+6x5+12x3+12x2+12x+40
*/

import java.util.Scanner;
import java.util.TreeMap;



public class Main {

	public static void main(String[] args) {
		Scanner in =new Scanner(System.in);
		int count = 2;
		Map<Integer, Integer> tree =new TreeMap<Integer,Integer>();
		int first,second;
		while(count>0)
		{
			first = in.nextInt();
			if(first==0)
			{
				count--;
			}
			try{
				second = in.nextInt();
			}catch (Exception e) {
				second = 0;
				//e.printStackTrace();
			}
			
			tree.put(first, second);
			
		}
		
	}

}
