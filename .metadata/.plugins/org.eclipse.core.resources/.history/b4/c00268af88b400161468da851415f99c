package mileageV2;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		CityHelper cp = new CityHelper();
		Scanner in =new Scanner(System.in);
		String cName = in.next();
		while(true)
		{
			if("###".equals(cName))
			{
				break;
			}
			else
			{
				cp.addCity(cName);
			}
		}
		int i =cp.getCityNum();
		int j =i;
		String mileage;
		for(;i>0;i--)
		{
			for(;j>0;j--)
			{
				mileage = in.nextLine();
				
			}
		}
		
		
		
	}

}
