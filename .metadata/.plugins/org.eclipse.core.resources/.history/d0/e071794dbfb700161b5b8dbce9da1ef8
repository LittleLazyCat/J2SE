package Duck;

public abstract class Duck {
	FlyBehavior flyBehavior;
	QuackBehavior quackBehavior;
	abstract void display();
	void performFly(){
		flyBehavior.fly();
	}
	void performQuack(){
		quackBehavior.quack();
	}
	void setFlyBehavior(FlyBehavior fly){
		flyBehavior = fly;
	}
	void setQuackBehavior(QuackBehavior quack)
	{
		quackBehavior = quack;
	}
}
