package human;

public class Student {
	String name;
	int Birth;
	String sex;
	public Student(){
		
	}
	public Student(String name, int birth, String sex) {
		super();
		this.name = name;
		Birth = birth;
		this.sex = sex;
	}

	
	
	@Override
	public String toString() {
		return "Student [name=" + name + ", Birth=" + Birth + ", sex=" + sex + "]";
	}
	public Student[] stuSort(Student[] s)
	{
		Student stu = null;
		int len = s.length;
		//Ñ¡ÔñÅÅĞò
		for(int i =0;i<len;i++)
		{	
			int min = i;
			stu = s[i];
			for(int j=i+1;j<len;j++)
			{
				if(s[j].Birth>stu.Birth)
				{
					stu = s[j];
					min = j;
				}
				
			}
			if(min!=i)
			{
				s[min] = s[i];
				s[i] = stu;
			}
		}
		return s;
	}
	public static void main(String[] args) {
		Student[] student={
			    new Student ("ÁÖ³ş½ğ",1989 ,"ÄĞ"),
			    new Student ("Öî¸ğÁÁ",1981,"ÄĞ"),
			    new Student ("Âõ¿Ë½Ü¿ËÑ·",1958 ,"ÄĞ"),
			    new Student ("ÇÇµ¤",1963 ,"ÄĞ"),
			    new Student ("ÄÃÆÆÂØ",1769 ,"ÄĞ"),
			      new Student ("²Ô¾®¿Õ",1983,"Å®")};
		new Student().stuSort(student);
		for(int i=0;i<student.length;i++)
		{
			System.out.println(student[i]);
		}
	}

}
